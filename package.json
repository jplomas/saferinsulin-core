    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oxlint": "^0.9.9",
    "prettier": "^2.8.8",
    "rollup": "^4.5.0",
